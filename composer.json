{
  "name": "yproximite/influxdb-preset-bundle",
  "description": "InfluxDbPresetBundle: send metrics to InfluxDB server based on Events",
  "keywords": ["influx", "db", "influxdb", "statistics", "metrics"],
  "type": "symfony-bundle",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Yproximite/Symfony2-coding-standard"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/framework-bundle": "^2.8 || ^3.0",
    "algatux/influxdb-bundle": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "Yproximite\\Bundle\\InfluxDbPresetBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpspec/phpspec": "^3.2",
    "m6web/Symfony2-coding-standard": "dev-master",
    "m6web/coke": "^2.1"
  },
  "config": {
    "bin-dir": "bin"
  }
}
