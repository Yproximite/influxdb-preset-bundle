{
  "name": "yproximite/influxdb-preset-bundle",
  "description": "InfluxDbPresetBundle: send metrics to InfluxDB server based on Events",
  "keywords": ["influx", "db", "influxdb", "statistics", "metrics"],
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^5.4 || ^6.0",
    "yproximite/influxdb-bundle": "^4.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^7.2",
    "friendsofphp/php-cs-fixer": "^3.8",
    "phpstan/phpstan": "^1.6",
    "phpstan/phpstan-strict-rules": "^1.2",
    "phpstan/phpstan-symfony": "^1.1"
  },
  "minimum-stability": "beta",
  "autoload": {
    "psr-4": {
      "Yproximite\\Bundle\\InfluxDbPresetBundle\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
