{
  "name": "yproximite/influxdb-preset-bundle",
  "description": "InfluxDbPresetBundle: send metrics to InfluxDB server based on Events",
  "keywords": ["influx", "db", "influxdb", "statistics", "metrics"],
  "type": "symfony-bundle",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Yproximite/influxdb-bundle"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^5.4 || ^6.0",
    "algatux/influxdb-bundle": "^3.1"
  },
  "require-dev": {
    "phpspec/phpspec": "^7.2",
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "minimum-stability": "beta",
  "autoload": {
    "psr-4": {
      "Yproximite\\Bundle\\InfluxDbPresetBundle\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
