{
  "name": "yproximite/influxdb-preset-bundle",
  "description": "InfluxDbPresetBundle: send metrics to InfluxDB server based on Events",
  "keywords": ["influx", "db", "influxdb", "statistics", "metrics"],
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "symfony/framework-bundle": "^4.4||^5.0",
    "algatux/influxdb-bundle": "~2.1||3.0.0-beta-1"
  },
  "autoload": {
    "psr-4": {
      "Yproximite\\Bundle\\InfluxDbPresetBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpspec/phpspec": "^6.1",
    "friendsofphp/php-cs-fixer": "^2.16.1",
    "symfony/profiler-pack": "^1.0"
  },
  "config": {
    "bin-dir": "bin"
  }
}
